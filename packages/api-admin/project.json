{
  "name": "api-admin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/api-admin/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/api-admin/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/api-admin"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "contracts": {
      "executor": "nx:run-commands",
      "dependsOn": ["generator:build:bin"],
      "options": {
        "command": "ts-node-esm -r tsconfig-paths/register scripts/generateAdminTypes.ts",
        "cwd": "packages/api-admin"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/packages/api-admin"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/api-admin"
      }
    }
  }
}
